<?xml version="1.0" encoding="UTF-8"?>
<payment-processors xmlns="http://www.demandware.com/xml/impex/paymentprocessor/2007-03-31">
    <payment-processor processor-id="ADYEN_CREDIT">
        <description>adyen credit card payment procesor</description>
        <preferences>
            <all-instances/>
            <development/>
            <staging/>
            <production/>
        </preferences>
    </payment-processor>

    <payment-processor processor-id="Adyen">
        <description>redirect to Adyen payment page</description>
        <preferences>
            <all-instances/>
            <development>
                <preference preference-id="Adyen_merchantCode">JosephFashionLtdCOM</preference>
                <preference preference-id="Adyen_skinCode">EhYCKQEZ</preference>
                <preference preference-id="Adyen_HMACkey">SampleHMAC</preference>
                <preference preference-id="Adyen_allowedMethods">mc,visa,amex,maestrouk,solo,ideal,elv,paypal,sepadirectdebit,bankTransfer_IBAN</preference>
                <preference preference-id="Adyen_PaymentSelection">one</preference>
                <preference preference-id="Adyen_Mode">TEST</preference>
                <preference preference-id="Adyen_Debug">true</preference>
                <preference preference-id="adyen_notification_password">adyen$</preference>
                <preference preference-id="adyen_notification_user">AdyenUser</preference>
                <preference preference-id="Adyen_directoryLookup">false</preference>
            </development>
            <staging/>
            <production/>
        </preferences>
    </payment-processor>

    <payment-processor processor-id="BASIC_GIFT_CERTIFICATE">
        <description>Internal gift certificate handling.</description>
        <preferences>
            <all-instances/>
            <development/>
            <staging/>
            <production/>
        </preferences>
    </payment-processor>

    <payment-processor processor-id="PAYPAL_CREDIT">
        <description>Paypal online credit card authorization (test and production systems).</description>
        <preferences>
            <all-instances/>
            <development/>
            <staging/>
            <production/>
        </preferences>
    </payment-processor>

    <payment-processor processor-id="PAYPAL_EXPRESS">
        <description>Paypal Express Checkout (test and production systems).</description>
        <preferences>
            <all-instances/>
            <development/>
            <staging/>
            <production/>
        </preferences>
    </payment-processor>

</payment-processors>
